{
    "name":  "xenomorph-park-react",
    "version":  "1.0.0",
    "type":  "module",
    "private":  true,
    "scripts":  {
                    "dev":  "vite",
                    "build":  "tsc -b \u0026\u0026 vite build",
                    "lint":  "eslint .",
                    "lint:fix":  "eslint . --fix",
                    "format":  "prettier --write .",
                    "format:check":  "prettier --check .",
                    "preview":  "vite preview",
                    "type-check":  "tsc --noEmit",
                    "test":  "vitest --configLoader runner",
                    "test:run":  "vitest run --configLoader runner",
                    "test:coverage":  "vitest run --coverage",
                    "test:ui":  "vitest --ui",
                    "ci":  "npm run lint \u0026\u0026 npm run type-check \u0026\u0026 npm run format:check \u0026\u0026 npm run test:run \u0026\u0026 npm run build",
                    "ci:quick":  "npm run lint \u0026\u0026 npm run type-check \u0026\u0026 npm run format:check"
                },
    "dependencies":  {
                         "@emotion/is-prop-valid":  "^1.4.0",
                         "@tailwindcss/vite":  "^4.1.10",
                         "autoprefixer":  "^10.4.21",
                         "framer-motion":  "^12.18.1",
                         "react":  "^19.1.0",
                         "react-dom":  "^19.1.0",
                         "react-router-dom":  "^7.6.2",
                         "react-use":  "^17.6.0",
                         "tailwindcss":  "^4.1.10",
                         "zustand":  "^5.0.5"
                     },
    "devDependencies":  {
                            "@eslint/js":  "^9.25.0",
                            "@types/react":  "^19.1.2",
                            "@types/react-dom":  "^19.1.2",
                            "@vitejs/plugin-react":  "^4.4.1",
                            "eslint":  "^9.25.0",
                            "eslint-plugin-react-hooks":  "^5.2.0",
                            "eslint-plugin-react-refresh":  "^0.4.19",
                            "globals":  "^16.0.0",
                            "typescript":  "~5.8.3",
                            "typescript-eslint":  "^8.30.1",
                            "vite":  "^6.3.5",
                            "vitest":  "^3.2.4",
                            "@testing-library/jest-dom":  "^6.8.0",
                            "@testing-library/react":  "^16.3.0",
                            "@testing-library/user-event":  "^14.6.1"
                        }
}
